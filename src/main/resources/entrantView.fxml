<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="400.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EntrantController">
   <children>
      <Label prefHeight="30.0" text="Окно добавления или удаления абитуриента" />
      <TextField fx:id="nameInput" prefHeight="30.0" promptText="Введите ФИО абитуриента" />
      <TextField fx:id="scoreInput" prefHeight="30.0" promptText="Балл" />
      <TextField fx:id="idpasportInput" prefHeight="30.0" promptText="Введите номер паспорта" />
      <TextField fx:id="passportSeriesInput" prefHeight="30.0" promptText="Введите серию паспорта" />
      <TextField fx:id="sexInput" prefHeight="30.0" promptText="Введите пол" />
      <TextField fx:id="registrationInput" prefHeight="30.0" promptText="Введите гражданство" />
      <TextField fx:id="citizenshipInput" prefHeight="30.0" promptText="Введите прописку" />
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Button fx:id="addButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Добавить" />
            <Button fx:id="deleteButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Удалить" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
